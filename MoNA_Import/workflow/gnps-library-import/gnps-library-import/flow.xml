<?xml version="1.0" encoding="ISO-8859-1" ?>
<flow name="gnps-stats">
        <object     name="flowParams"/>
        <collection     name="mona_update"/>

        <action name="begin">
                <output port="flowParams" object="flowParams"/>
        </action>

        <action name="mona_import">
                <input  port="flowParams"             object="flowParams"/>
                <output port="mona_update"           collection="mona_update"/>
        </action>

        <action name="end">
                <input port="mona_update"           collection="mona_update"/>
        </action>
</flow>
